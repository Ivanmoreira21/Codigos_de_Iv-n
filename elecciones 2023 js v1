
//Asumiendo que tenemos 4 postulantes.
let postulantes = 3;
console.log()

//valores predeterminados para los calculos
let favor=4;
let en_contra=-1;
let blanco=0;
let acumulador=0;

for (i = 1; i <= postulantes; i++) {
  // Ingresos para solicitar respuestas
  console.log("--Posultante #NÃºmero: ", i)
  Correctas=5;
  Incorrectas=6;
  Blanco=3;
  

  puntajefinal=(Correctas *favor)+(Incorrectas*en_contra)+(Blanco*blanco)
  console.log("puntaje final=", puntajefinal)
  console.log("___________________________")
  console.log()
  acumulador=acumulador+puntajefinal
}

console.log("Con",[postulantes],"entrevistados/postulantes se alcanza un puntaje final total de",[acumulador] )



